{
    "name": "laminas/laminas-oauth",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "oauth"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "issues": "https://github.com/laminas/laminas-oauth/issues",
        "source": "https://github.com/laminas/laminas-oauth",
        "rss": "https://github.com/laminas/laminas-oauth/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "laminas/laminas-config": "^2.0",
        "laminas/laminas-crypt": "^2.0",
        "laminas/laminas-http": "^2.0",
        "laminas/laminas-i18n": "^2.0",
        "laminas/laminas-loader": "^2.0",
        "laminas/laminas-math": "^2.0",
        "laminas/laminas-stdlib": "^2.7.8",
        "laminas/laminas-uri": "^2.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~1.0.0",
        "phpunit/phpunit": "^5.7.27 || ^6.5.14 || ^7.5.20"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\OAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\OAuth\\": "test/"
        }
    },
    "scripts": {
        "static-analysis": "psalm --shepherd --stats"
    },
    "replace": {
        "zendframework/zendoauth": "^2.0.3"
    }
}
